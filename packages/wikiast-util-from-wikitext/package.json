{
  "name": "wikiast-util-from-wikitext",
  "version": "0.2.0",
  "description": "Get Tiddlywiki wikiast from wikitext, by simply calling `$tw.wiki.parseText`",
  "type": "module",
  "main": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin swc3",
    "clean": "rimraf dist",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -c ./jest.config.ts",
    "generate-barrels": "barrelsby --delete --location all --singleQuotes --directory ./src --exclude .+\\.d\\.ts && dprint fmt **/*/index.ts",
    "prepublish": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiddly-gittly/wikiast.git",
    "directory": "packages/wikiast-util-from-wikitext"
  },
  "files": [
    "dist/**/*.mjs",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "keywords": [
    "openai",
    "chatgpt"
  ],
  "author": "linonetwo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiddly-gittly/wikiast/issues"
  },
  "homepage": "https://github.com/tiddly-gittly/wikiast#readme",
  "dependencies": {},
  "devDependencies": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true
  }
}
